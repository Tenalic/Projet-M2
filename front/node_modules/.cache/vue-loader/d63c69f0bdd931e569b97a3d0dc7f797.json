  </v-btn>\n        </div>\n\n        <!-- UTILISATEUR NON CONNECTE -->\n        <div v-else>\n          <!-- Se connecter -->\n          <v-btn color = \"primary\"\n          elevation = 0\n          to=\"Login\">\n            <span class=\"btn\">Se connecter</span>\n          </v-btn>\n          <!-- Inscription -->\n          <v-btn color = \"primary\"\n          elevation = 0\n          to=\"Signup\">\n            <span class=\"btn\">Inscription</span>\n          </v-btn>\n        </div>\n      </div>\n      <!-- FIN PARTIE DROITE -->\n    </v-app-bar>\n</div>\n</template>\n\n<script>\nexport default {\n  name: 'AppBar',\n  computed: {\n    // Retourne si l'utilisateur est connect√©\n    isUserAuthenticated () {\n      return (\n        this.$store.getters.user !== null &&\n        this.$store.getters.user !== undefined\n      )\n    },\n    // Retourne le nom de l'utilisateur\n    getUsername () {\n      return (\n        this.$store.getters.user\n      )\n    }\n  }\n}\n</script>\n"]}]}